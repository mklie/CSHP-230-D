@model HomePage.Models.CourseModel[]

@{
    ViewBag.Title = "Courses";
}
<table class="table">
    <tr>
        <th>
            @Html.Label("Name")
        </th>
        <th>
            @Html.Label("Description")
        </th>
        <th>
            @Html.Label("Price")
        </th>

        <th></th>
    </tr>
    @foreach (var p in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => p.ClassName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => p.ClassDescription)
            </td>
            <td>
                @Html.DisplayFor(modelItem => p.ClassPrice)
            </td>
        </tr>

    }
    </table> 